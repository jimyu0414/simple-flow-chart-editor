{
  "name": "flow-chart",
  "version": "1.0.0",
  "description": "boosted by express backend  and react front end",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "build": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimyu0414/flow-chart.git"
  },
  "keywords": [
    "express",
    "react"
  ],
  "author": "Yu Liu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jimyu0414/flow-chart/issues"
  },
  "homepage": "https://github.com/jimyu0414/flow-chart#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^5.0.0",
    "express": "^4.18.2",
    "nodemon": "^1.19.4"
  }
}
